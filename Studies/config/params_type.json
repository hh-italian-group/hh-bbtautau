{
    "num_den_layers_pre":{
        "range" : [0, 5],
        "type": "int"
    },
    "num_units_den_layers_pre":{
        "range" : [15, 60],
        "type": "int",
        "depends_on": "num_den_layers_pre"
    },
    "dropout_rate_den_layers_pre":{
        "range" : [0, 0.5],
        "type": "float",
        "depends_on": "num_den_layers_pre"
    },
    "num_den_layers_post":{
        "range" : [0, 5],
        "type": "int"
    },
    "num_units_den_layers_post":{
        "range" : [15, 60],
        "type": "int",
        "depends_on": "num_den_layers_post"
    },
    "dropout_rate_den_layers_post":{
        "range" : [0, 0.5],
        "type": "float",
        "depends_on": "num_den_layers_post"
    },
    "num_rnn_layers":{
        "range" : [3, 10],
        "type": "int"
    },
    "num_units_rnn_layer":{
